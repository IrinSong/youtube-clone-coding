extends ../layout
include ../mixins/video

block navLink
  li 
    a(href="/") Home
  li
    a(href="/search") Search

block content 
  if !loggedIn
  else if String(loggedInUser._id) === String(id)
    a(href="/users/edit") Edit profile
  hr
  each video in user.videos
    +info(video)
  else
   li Sorry nothing found
  hr