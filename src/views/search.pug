extends layout
include mixins/video

block navLink
  li
    a(href="/") Home
  li
    a(href="/videos/upload") Upload Video
        
block content
  form(method="GET")
    input(type="text" name="keyword" placeholder="Search by title")
    input(type="submit" value="Search Now")
  
  div
    each video in videos 
      +info(video)